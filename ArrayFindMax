#include <iostream>
using namespace std;

int main() {
    int arr[5];
    int sum = 0;
    int max;

    for (int i = 0; i < 5; i++) {
        cout << "enter a number :" << endl;
        cin >> arr[i];
        sum = sum + arr[i];
    }

    max = arr[0];
    for (int i = 1; i < 5; i++) {
        if (max < arr[i]) {
            max = arr[i];
        }
    }

    cout << "the total sum :" << sum << endl;
    cout << "the maximum number is =" << max;
    
    return 0;
}
