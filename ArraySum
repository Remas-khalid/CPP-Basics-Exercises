#include <iostream>
using namespace std;

int main() {
    int arr[5];
    int sum = 0;

    for (int i = 0; i < 5; i++) {
        cout << "enter a number :";
        cin >> arr[i];
    }

    for (int i = 0; i < 5; i++) {
        sum = sum + arr[i];
    }

    cout << "the total sum :" << sum << endl;
    
    return 0;
}
